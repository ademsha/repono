{
  "amazon":{
    "search_pattern":"I just bought via @amazon",
    "extraction_patterns":{
      "title":"I just bought: '(.*) via",
      "url":".*?(amazon.com/dp/[0-9A-Za-z].*)\/",
      "url_id":".*?/dp/([0-9A-Za-z]*)\/"
    }
  },
  "youtube":{
    "search_pattern":"I liked @YouTube",
    "extraction_patterns":{
      "title":".*t.co/ (.*)$",
      "url":".*?(youtu.be/[0-9A-Za-z]*)",
      "author":"from @(.*) http://"
    }
  },
  "yelp-reviews":{
    "search_pattern":"Review on #Yelp",
    "extraction_patterns":{
      "place":"(.*) on #Yelp:",
